{
  "name": "@wsh-2025/client",
  "private": true,
  "scripts": {
    "build": "wireit",
    "format": "wireit",
    "format:eslint": "wireit",
    "format:prettier": "wireit"
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.15",
    "@dhmk/zustand-lens": "5.0.0",
    "@epic-web/restore-scroll": "1.1.1",
    "@headlessui/react": "2.2.0",
    "@radix-ui/react-slider": "1.2.3",
    "@standard-schema/spec": "1.0.0",
    "@types/webpack-bundle-analyzer": "4.7.0",
    "@wsh-2025/client": "workspace:*",
    "@wsh-2025/schema": "workspace:*",
    "final-form": "4.20.10",
    "immer": "10.1.1",
    "luxon": "3.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-final-form": "6.5.9",
    "react-flip-toolkit": "7.2.4",
    "react-router": "7.0.2",
    "react-router-dom": "7.0.2",
    "react-use": "17.6.0",
    "setimmediate": "1.0.5",
    "tiny-invariant": "1.3.3",
    "type-fest": "4.29.1",
    "use-callback-ref": "1.3.3",
    "use-sync-external-store": "1.4.0",
    "valibot": "1.0.0-rc.3",
    "view-transitions-polyfill": "1.0.3",
    "zod": "3.24.1",
    "zustand": "5.0.3",
    "zustand-di": "0.0.16"
  },
  "devDependencies": {
    "@iconify/json": "2.2.317",
    "@iconify/types": "2.0.0",
    "@rspack/cli": "1.3.2",
    "@rspack/core": "1.3.2",
    "@types/luxon": "3.4.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/wicg-task-scheduling": "2024.1.0",
    "@unocss/eslint-config": "66.1.0-beta.8",
    "@unocss/reset": "66.1.0-beta.5",
    "@unocss/webpack": "66.1.0-beta.7",
    "@wsh-2025/configs": "workspace:*",
    "arraybuffer-loader": "1.0.8",
    "core-js": "3.41.0",
    "css-loader": "7.1.2",
    "hls.js": "1.5.17",
    "typescript": "5.7.2",
    "unocss": "66.1.0-beta.7",
    "webpack-bundle-analyzer": "4.10.2",
    "wireit": "0.14.9"
  },
  "wireit": {
    "build": {
      "command": "rspack build",
      "files": [
        "../../pnpm-lock.yaml",
        "../schema/src/**/*",
        "./src/**/*",
        "./rspack.config.mjs"
      ],
      "output": [
        "./dist/**/*"
      ]
    },
    "format": {
      "command": "pnpm run format:eslint && pnpm run format:prettier"
    },
    "format:eslint": {
      "command": "eslint --fix ."
    },
    "format:prettier": {
      "command": "prettier --write ."
    }
  }
}